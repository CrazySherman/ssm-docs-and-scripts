{
    "schemaVersion": "1.2",
    "description": "This document defines the PowerShell command to run or path to a script which is to be executed.",
    "runtimeConfig": {
      "aws:runScript": {
        "Properties": [
          {
            "id": "0.aws:runScript",
            "runCommand": "{{ runCommand }}",
            "timeoutSeconds": "{{ timeoutSeconds }}",
            "workingDirectory": "{{ workingDirectory }}"
          }
        ]
      }
    }
  }
